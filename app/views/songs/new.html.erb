<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.collection_select :genre, Genre.all, :name, :name%>

  <input name="song[note_contents][]" id="notes_1">
  <input name="song[note_contents][]" id="notes_1">
  <br />

  <%= f.submit %>
<% end %>
